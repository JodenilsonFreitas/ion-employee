<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="@@androidVersion" id="@@id" version="@@version" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>@@name</name>
    <description>@@description</description>
    <author email="contact@meumobi.com" href="http://meumobi.com">meumobi Team</author>
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-installLocation" value="auto" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="detect-data-types" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="orientation" value="default" />
    <preference name="permissions" value="none" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <plugin name="onesignal-cordova-plugin" spec="~2.0.11" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
    <plugin name="cordova-plugin-whitelist" spec="1.3.1"/>
    <plugin name="cordova-plugin-console" spec="1.0.5"/>
    <plugin name="cordova-plugin-statusbar" spec="2.2.2"/>
    <plugin name="cordova-plugin-device" spec="1.1.4"/>
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.1"/>
    <icon src="icon.png" />
    <splash src="splash.png" />
    <platform name="android">
        <icon qualifier="ldpi" src="res/icon/android/mipmap-ldpi/ic_launcher.png" />
        <icon qualifier="mdpi" src="res/icon/android/mipmap-mdpi/ic_launcher.png" />
        <icon qualifier="hdpi" src="res/icon/android/mipmap-hdpi/ic_launcher.png" />
        <icon qualifier="xhdpi" src="res/icon/android/mipmap-xhdpi/ic_launcher.png" />
        <icon qualifier="xxhdpi" src="res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
        <icon qualifier="xxxhdpi" src="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />
        <splash qualifier="land-ldpi" src="res/screen/android/drawable-land-ldpi-screen.png" />
        <splash qualifier="land-mdpi" src="res/screen/android/drawable-land-mdpi-screen.png" />
        <splash qualifier="land-hdpi" src="res/screen/android/drawable-land-hdpi-screen.png" />
        <splash qualifier="land-xhdpi" src="res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash qualifier="land-xxhdpi" src="res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash qualifier="land-xxxhdpi" src="res/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash qualifier="port-ldpi" src="res/screen/android/drawable-port-ldpi-screen.png" />
        <splash qualifier="port-mdpi" src="res/screen/android/drawable-port-mdpi-screen.png" />
        <splash qualifier="port-hdpi" src="res/screen/android/drawable-port-hdpi-screen.png" />
        <splash qualifier="port-xhdpi" src="res/screen/android/drawable-port-xhdpi-screen.png" />
        <splash qualifier="port-xxhdpi" src="res/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash qualifier="port-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <icon height="180" src="res/icon/ios/AppIcon.appiconset/Icon-App-60x60@3x.png" width="180" />
        <icon height="60" src="res/icon/ios/AppIcon.appiconset/Icon-App-60x60@1x.png" width="60" />
        <icon height="120" src="res/icon/ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" />
        <icon height="76" src="res/icon/ios/AppIcon.appiconset/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="res/icon/ios/AppIcon.appiconset/Icon-App-76x76@2x.png" width="152" />
        <icon height="40" src="res/icon/ios/AppIcon.appiconset/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="res/icon/ios/AppIcon.appiconset/Icon-App-40x40@2x.png" width="80" />
        <icon height="57" src="res/icon/ios/AppIcon.appiconset/Icon-App-57x57@1x.png" width="57" />
        <icon height="114" src="res/icon/ios/AppIcon.appiconset/Icon-App-57x57@2x.png" width="114" />
        <icon height="72" src="res/icon/ios/AppIcon.appiconset/Icon-App-72x72@1x.png" width="72" />
        <icon height="144" src="res/icon/ios/AppIcon.appiconset/Icon-App-57x57@2x.png" width="144" />
        <icon height="29" src="res/icon/ios/AppIcon.appiconset/Icon-App-29x29@1x.png" width="29" />
        <icon height="58" src="res/icon/ios/AppIcon.appiconset/Icon-App-29x29@2x.png" width="58" />
        <icon height="50" src="res/icon/ios/AppIcon.appiconset/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="res/icon/ios/AppIcon.appiconset/Icon-Small-50x50@2x.png" width="100" />
        <icon height="167" src="res/icon/ios/AppIcon.appiconset/Icon-App-83.5x83.5@2x.png" width="167" />
        <splash height="1136" src="res/screen/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/screen/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/screen/ios/Default-736h.png" width="1242" />
        <splash height="1242" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1024" src="res/screen/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="768" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" src="res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="960" src="res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" src="res/screen/ios/Default~iphone.png" width="320" />
    </platform>
    <allow-navigation href="*" />
    <allow-navigation href="cdvfile://*" />
    <access origin="*" />
    <access origin="cdvfile://*" />
    <allow-intent href="file://*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="cdvfile://*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-intent href="ibooks:*" />
    </platform>
    <content src="index.html" />
</widget>
